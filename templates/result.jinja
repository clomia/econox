<html>

<head>
    <title>Item Details</title>
    <link href="/static/css/styles.css" rel="stylesheet">
</head>

<body>
    <div class="outer-box">
        {% for name, note in symbols %}
            <div class="box">
                <div class="inner-box">{{ name }}</div>
                <div class="inner-box">{{ note }}</div>
            </div>
        {% endfor %}
    </div>
    <div class="outer-box">
        {% for name, note in countries %}
            <div class="box">
                <div class="inner-box">{{ name }}</div>
                <div class="inner-box">{{ note }}</div>
            </div>
        {% endfor %}
    </div>
</body>

</html>